interface WrapperScaleInterface {
	List<DeploymentComponent> getDeploymentComponent();
	Unit deploy();
	Unit undeploy();
}
//it should be created with only one Scale object but
//if it is created with more than one Scale object, it considers only the first
class WrapperScale(Scale1 s1, Scale2 s2, Scale3 s3) implements WrapperScaleInterface{

	List<DeploymentComponent> getDeploymentComponent(){
		List<DeploymentComponent> res = Nil;
		if(s3 != null){
			List<DeploymentComponent> dcListS3 = s3.getDeploymentComponent();
			List<DeploymentComponent> dcListS2 = s2.getDeploymentComponent();
			List<DeploymentComponent> dcListS1 = s1.getDeploymentComponent();
			List<DeploymentComponent> dcListS3S2 = concatenate(dcListS3, dcListS2);
			res = concatenate(dcListS3S2, dcListS1);
		}
		else{
			if(s2 != null){
				List<DeploymentComponent> dcListS2 = s2.getDeploymentComponent();
				List<DeploymentComponent> dcListS1 = s1.getDeploymentComponent();
				res = concatenate(dcListS2, dcListS1);
			}
			else{
				res = s1.getDeploymentComponent();
			}
		}
		return res;
	}

	Unit deploy(){
		if(s3 != null){
			s1.deploy();
			s2.deploy();
			s3.deploy();
		}
		else{
			if(s2 != null){
				s1.deploy();
				s2.deploy();
			}
			else{
				s1.deploy();
			}
		}
	}

	Unit undeploy(){
		if(s3 != null){
			s1.undeploy();
			s2.undeploy();
			s3.undeploy();
		}
		else{
			if(s2 != null){
				s2.undeploy();
			}
			else{
				s1.undeploy();
			}
		}
	}

}


interface MonitorInterface {
	Unit insertScalingPolicy (Int scalingValue, WrapperScaleInterface scalingObj, String scalingName);
}
//this class has an active behaviour so the monitoring operations start when the object is created in an automatic way
class Monitor(DBInterface db) implements MonitorInterface {

	//dimension of the time windows for monitoring controls
	Int monitoringWindowsDimension = 100;
	//number of messagges supported by the initial configuration
	Int numberOfMessaggesActualSupported = 5;

	List<Int> scalingPolicyValues = list[];
	List<WrapperScaleInterface> scalingObjects = list[];
	List<String> scalingPolicyNames = list[];

	// inserting a new scaling policy mantaining the list ordered
	Unit insertScalingPolicy (Int scalingValue, WrapperScaleInterface scalingObj, String scalingName){

		if(isEmpty(scalingPolicyValues)){
			//the first scaling policy is simple inserted
			scalingPolicyValues = appendright(scalingPolicyValues, scalingValue);
			scalingObjects = appendright(scalingObjects, scalingObj);
			scalingPolicyNames = appendright(scalingPolicyNames, scalingName);
		}
		else{
			//otherwise we have to mantain the descending order respect the scaling value, finding the correct position for the new scaling object
			//we create two new list inserting the previous objects and the new one in the correct order
			List<Int> newScalingPolicyValues = list[];
			List<WrapperScaleInterface> newScalingObjects = list[];
			List<String> newScalingPolicyNames = list[];

			Bool inserted = False;
			Int i = 0;
			while (i < length(scalingPolicyValues)) {
				if(nth(scalingPolicyValues, i) < scalingValue && !inserted){
					newScalingPolicyValues = appendright(newScalingPolicyValues, scalingValue);
					newScalingObjects = appendright(newScalingObjects, scalingObj);
					newScalingPolicyNames = appendright(newScalingPolicyNames, scalingName);
					inserted = True;
				}
				newScalingPolicyValues = appendright(newScalingPolicyValues, nth(scalingPolicyValues, i));
				newScalingObjects = appendright(newScalingObjects, nth(scalingObjects, i));
				newScalingPolicyNames = appendright(newScalingPolicyNames, nth(scalingPolicyNames, i));
				i = i + 1;
			}
			//if we inserted all the old elements but not the new one, this means that the new scaling policy is the smallest so we insert it in the last position
			if(!inserted){
				newScalingPolicyValues = appendright(newScalingPolicyValues, scalingValue);
				newScalingObjects = appendright(newScalingObjects, scalingObj);
				newScalingPolicyNames = appendright(newScalingPolicyNames, scalingName);
				inserted = True;
			}

			//update the class corrisponding fields
			scalingPolicyValues = newScalingPolicyValues;
			scalingObjects = newScalingObjects;
			scalingPolicyNames = newScalingPolicyNames;
		}

	}

	// active behavior
	Unit run() {

		while(True){
			//wait the monitoring time
			duration(monitoringWindowsDimension,monitoringWindowsDimension);

			//require to the db the number of messages arrived in the monitorning windows
			Int numberOfMessaggesInTheMonitoringWindows = await db!returnNumberOfMessaggesInTheMonitoringWindows();
			//calculate the difference with the number of messages supported by the actual configuration
			Int difference = numberOfMessaggesInTheMonitoringWindows - numberOfMessaggesActualSupported;

			Bool notScaled = True;

			if(difference > 0){
				//check scale up
				//visiting the scaling policy list requiring all the needed scale operations to reach a configuration that support the number of messages received
				Int i = 0;
				//while we have possible scaling policy and the last one is smaller (and so necessary) respect to the difference request
				while (i < length(scalingPolicyValues) && difference >= nth(scalingPolicyValues, length(scalingPolicyValues)-1)) {
					if(difference >= nth(scalingPolicyValues, i)){
						//require the corrisponding scaling operation
						WrapperScaleInterface s = nth(scalingObjects, i);
						s.deploy();
						println("\t\t\t\t\t\t\t\tDifference = " + toString(difference) + " " + nth(scalingPolicyNames, i) + "-up");

						notScaled = False;
						numberOfMessaggesActualSupported = numberOfMessaggesActualSupported + nth(scalingPolicyValues, i);
						difference = difference - nth(scalingPolicyValues, i);
					}
					else{
						//otherwise pass to check if it is necessary the next scaling policy
						i = i + 1;
					}
				}
			}
			else{
				//check scale down
				Int i = 0;
				//while we have possible scaling policy and the last one is smaller (and so it could be necessary) respect to the difference request
				while (i < length(scalingPolicyValues) && difference <= -nth(scalingPolicyValues, length(scalingPolicyValues)-1)) {
					WrapperScaleInterface s = nth(scalingObjects, i);
					List<DeploymentComponent> dcList = s.getDeploymentComponent();
					//to check if the corrisponding scaling policy could be necessary
					Bool a = difference <= -nth(scalingPolicyValues, i);
					//to check if we used a corrisponding scale up policy to require, in that case, the opposite operation
					Bool b = dcList != Nil;
					if( a && b){
						//require the corrisponding scaling operation
						s.undeploy();
						println("\t\t\t\t\t\t\t\tDifference = " + toString(difference) + " " + nth(scalingPolicyNames, i) + "-down");

						notScaled = False;
						numberOfMessaggesActualSupported = numberOfMessaggesActualSupported - nth(scalingPolicyValues, i);
						difference = difference + nth(scalingPolicyValues, i);
					}
					else{
						//otherwise pass to check if it is necessary the next scaling policy
						i = i + 1;
					}
				}
			}

			if(notScaled){
				//no scale operation needed
				println("\t\t\t\t\t\t\t\tDifference = " + toString(difference) + " No scale operations needed");
			}

			//after monitor controls, reset the number of messages in the monitoring windows in the db
			await db!resetNumberOfMessaggesInTheMonitoringWindows();
		}

    }
}


