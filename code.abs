class MailGenerator(EntryPointInterface entryPoint) implements MailGeneratorInterface {

	// active behavior
	Unit run() {
		Int messageCounter = 0;
		List<Int> randomArguments = list[6, 11, 16, 21];
		Int index = 0;
		while(True){
			messageCounter = this.generateMessages(messageCounter, nth(randomArguments, index));
			index = (index + 1) % length(randomArguments);
		}
	}

	Int generateMessages(Int messageCounter, Int randomArgument) {
		Int timePassed = 0;
		while(timePassed < 100){
			entryPoint!newMessage("Message" + toString(messageCounter));
			messageCounter = messageCounter + 1;
			Int timeBetweenTwoConsecutiveMessagges = random(randomArgument);

			//wait some time between a message and the following one
			duration(timeBetweenTwoConsecutiveMessagges,timeBetweenTwoConsecutiveMessagges);
			timePassed = timePassed + timeBetweenTwoConsecutiveMessagges;
		}
		return messageCounter;
	}
}


